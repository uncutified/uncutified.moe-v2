---
import BaseHead from '../components/BaseHead.astro';
import Footer from '../components/Footer.astro';
import Header from '../components/Header.astro';
import Summary from '../components/Summary.astro';
import Contactcard from '../components/Contactcard.astro';
import Friendpic from '../components/Friendpic.astro';
import { SITE_DESCRIPTION, SITE_TITLE } from '../consts';
import Eighteight from '../components/Eighteight.astro';
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">

	</head>
	<body>
		<Header />
		<main>
			<div class="subsplash">
				<div class="splash-content">
				  <div class="splash-header">
					<h2>88x31</h2>
				  </div>
				</div>
			  </div>
			  <hr class="section-seperation">
			  <div class="eighteightshow">
				<p>Animated button:</p>
				<img src="/88x31.gif" alt="">
              <br>
              <code class="codeblock">&lt;img src="https://uncutified.moe/88x31.gif"&gt;</code>
			  </div>

			  <div class="eighteightshow">
				<p>Non-Animated button:</p>
				<img src="/88x31.png" alt="">
              <br>
              <code class="codeblock">&lt;img src="https://uncutified.moe/88x31.png"&gt;</code>
			  </div>

				

			  <hr class="section-seperation">
		</main>
		<Footer />
	</body>
</html>